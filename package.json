{
  "name": "template-nx-project",
  "version": "0.0.0",
  "license": "GPL-3",
  "author": "Denis Zholob",
  "description": "template_nx_project_description",
  "repository": "github:deniszholob/template-nx-project",
  "private": true,
  "engines": {
    "node": ">= 18.19.1",
    "npm": ">= 8.19.2"
  },
  "scripts": {
    "i": "pnpm install",
    "nx:clear-cache": "nx reset",
    "storybook": "nx run template-nx-project:storybook",
    "start": "nx run template-nx-project:serve --port=4200",
    "start:prod": "pnpm run build && http-server -g -p 5555 --cors -c-1 dist/browser -o",
    "dep-graph": "nx dep-graph",
    "inferred-tasks": "nx show project template-nx-project --web ",
    "lint": "nx lint",
    "lint:fix": "nx lint --fix",
    "lint:err": "nx lint --quiet",
    "format:fix": "nx format:write",
    "format:check": "nx format:check",
    "affected:lint": "nx affected:lint",
    "affected:lint:fix": "nx affected:lint --fix",
    "affected:test": "nx affected:test",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "test": "nx test --codeCoverage",
    "test:view-coverage": "node -e \"require('child_process').exec(process.platform === 'win32' ? 'start' : process.platform === 'darwin' ? 'open' : 'xdg-open' + ' coverage/index.html')\"",
    "e2e:open": "nx e2e template-nx-project-e2e --port 8080 --watch=true",
    "e2e:ci": "nx e2e template-nx-project-e2e --env.grepTags='' --browser chrome",
    "e2e:integration": "nx e2e --env.grepTags='@integration' --browser chrome",
    "patchModifyDate": "node .github/ci/date-patch.js",
    "build": "nx build",
    "build:gh-prod": "pnpm run patchModifyDate && nx build template-nx-project --base-href='/template-nx-project/'",
    "pnpmVersionPatch": "pnpm version patch",
    "upgrade:nx:create": "nx migrate latest",
    "upgrade:nx:apply": "nx migrate --run-migrations",
    "upgrade:storybook": "npx storybook@latest upgrade --config-dir ./.storybook",
    "upgrade:storybook-auto": "npx storybook@latest automigrate --config-dir ./.storybook",
    "pre-commit": "npx lint-staged",
    "prepare": "husky install",
    "nx": "nx",
    "set-permissions": "chmod +x ./node_modules/husky/lib/bin.js && chmod +x ./node_modules/.bin/prettier"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@angular/animations": "18.2.2",
    "@angular/common": "18.2.2",
    "@angular/compiler": "18.2.2",
    "@angular/core": "18.2.2",
    "@angular/forms": "18.2.2",
    "@angular/platform-browser": "18.2.2",
    "@angular/platform-browser-dynamic": "18.2.2",
    "@angular/router": "18.2.2",
    "@babel/runtime": "^7.23.9",
    "@nx/angular": "19.6.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.2",
    "@angular-devkit/core": "18.2.2",
    "@angular-devkit/schematics": "18.2.2",
    "@angular-eslint/eslint-plugin": "18.3.0",
    "@angular-eslint/eslint-plugin-template": "18.3.0",
    "@angular-eslint/template-parser": "18.3.0",
    "@angular/cli": "~18.2.0",
    "@angular/compiler-cli": "18.2.2",
    "@angular/language-service": "18.2.2",
    "@nx/cypress": "19.6.4",
    "@nx/eslint": "19.6.4",
    "@nx/eslint-plugin": "19.6.4",
    "@nx/jest": "19.6.4",
    "@nx/js": "19.6.4",
    "@nx/storybook": "19.6.4",
    "@nx/web": "19.6.4",
    "@nx/workspace": "19.6.4",
    "@schematics/angular": "18.2.2",
    "@storybook/addon-a11y": "^8.2.9",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/angular": "^8.2.9",
    "@storybook/core-common": "^8.2.9",
    "@storybook/core-server": "^8.2.9",
    "@storybook/manager-api": "^8.2.9",
    "@storybook/theming": "^8.2.9",
    "@storybook/types": "^8.2.9",
    "@tailwindcss/forms": "^0.5.7",
    "@types/jest": "29.5.12",
    "@types/node": "^18.16.9",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "cypress": "13.13.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-cypress": "2.13.4",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-preset-angular": "14.1.0",
    "lint-staged": "^15.2.0",
    "ng-mocks": "^14.13.0",
    "nx": "19.6.4",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "storybook": "^8.2.9",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.5.4"
  }
}
